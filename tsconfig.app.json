{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "src/**/*-specs.ts",
    "src/**/*.avoid.ts",
    "src/**/*.0.ts",
    "src/**/*.1.ts",
    "src/**/*.1b.ts",
    "src/**/*.2.ts",
    "src/**/*.3.ts",
    "src/**/*.4.ts",
    "src/**/*.5.ts",
    "src/**/*.6.ts",
    "src/**/*.7.ts",
    "src/**/testing"
  ]
}
